@prefix : <http://www.semanticweb.org/progettomodsem2020/deliverydoctor#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <http://www.semanticweb.org/progettomodsem2020/deliverydoctor> .

<http://www.semanticweb.org/progettomodsem2020/deliverydoctor> rdf:type owl:Ontology ;
                                                                <http://purl.org/dc/elements/1.1/creator> "Lorenzo Sciandra"^^xsd:string ,
                                                                                                          "Stefano Vittorio Porta"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/date> "2020-12-29T18:30:10"^^xsd:dateTime ;
                                                                <http://purl.org/dc/elements/1.1/description> "Ontologia che tratta esercizi alimentari, pietanze servite ed interazione con utenti al fine dell'ordine, consegna e consumo di queste in precisi luoghi geografici."@it ;
                                                                <http://purl.org/dc/elements/1.1/language> "italiano"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/subject> "food delivery"^^xsd:string ,
                                                                                                          "ontology"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/title> "Delivery Doctor"^^xsd:string ;
                                                                vann:preferedNamespacePrefix "deliverydr"^^xsd:string ;
                                                                vann:preferredNamespaceUri "http://www.semanticweb.org/progettomodsem2020/deliverydoctor"^^xsd:string ;
                                                                rdfs:label "deliverydr"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/language
<http://purl.org/dc/elements/1.1/language> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/subject
<http://purl.org/dc/elements/1.1/subject> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/type
<http://purl.org/dc/elements/1.1/type> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferedNamespacePrefix
vann:preferedNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#localeSituatoIn
:localeSituatoIn rdf:type owl:ObjectProperty ;
                 rdfs:domain :EsercizioAlimentare ;
                 rdfs:range :LuogoGeografico ;
                 rdfs:comment "Property that allows a food establishment to be geographically and uniquely localized"@en ,
                              "Proprietà che permette di localizzare geograficamente ed univocamente un esercizio alimentare"@it ;
                 rdfs:label "Local Located In"@en ,
                            "Locale Situato In"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#utilizzaIngrediente
:utilizzaIngrediente rdf:type owl:ObjectProperty ;
                     owl:inverseOf :utilizzatoInPietanza ;
                     rdfs:domain :Pietanza ;
                     rdfs:range :ingrediente ;
                     rdfs:comment "Property that binds the food preparation with the set of simple or derivative ingredients that compose it."@en ,
                                  "Proprietà che lega la pietanza con l'insieme degli ingredienti semplici o derivati che la compongono."@it ;
                     rdfs:label "Use Ingredient"@en ,
                                "Utilizza Ingrediente"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#utilizzatoInPietanza
:utilizzatoInPietanza rdf:type owl:ObjectProperty ;
                      rdfs:domain :ingrediente ;
                      rdfs:range :Pietanza ;
                      rdfs:comment "Property that binds the ingredients to the food preparations in which they are used"@en ,
                                   "Proprietà che lega gli ingredienti alle pietanze in cui sono utilizzati"@it ;
                      rdfs:label "Used In Food Preparation"@en ,
                                 "Utilizzato In Pietanza"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#haAbitanti
:haAbitanti rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Città ;
            rdfs:range xsd:positiveInteger ;
            rdfs:comment "Property that specifies the number of inhabitants of a city"@en ,
                         "Proprietà che specifica il numero di abitanti di una città"@it ;
            rdfs:label "Ha Abitanti"@it ,
                       "Has Inhabitants"@en .


#################################################################
#    Classes
#################################################################

###  http://schema.org/BarOrPub
<http://schema.org/BarOrPub> rdf:type owl:Class ;
                             owl:equivalentClass :Bar ;
                             rdfs:subClassOf <http://schema.org/FoodEstablishment> .


###  http://schema.org/City
<http://schema.org/City> rdf:type owl:Class ;
                         owl:equivalentClass :Città ;
                         rdfs:subClassOf <http://schema.org/Place> ;
                         owl:disjointWith :Piazza ,
                                          :Via .


###  http://schema.org/FoodEstablishment
<http://schema.org/FoodEstablishment> rdf:type owl:Class ;
                                      owl:equivalentClass :EsercizioAlimentare ;
                                      rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
                                      owl:disjointWith :Pietanza ,
                                                       :Recensione .


###  http://schema.org/Place
<http://schema.org/Place> rdf:type owl:Class ;
                          owl:equivalentClass :LuogoGeografico ;
                          owl:disjointWith <http://www.w3.org/ns/prov#Agent> ,
                                           <http://www.w3.org/ns/prov#Entity> .


###  http://schema.org/Restaurant
<http://schema.org/Restaurant> rdf:type owl:Class ;
                               owl:equivalentClass :Ristorante ;
                               rdfs:subClassOf <http://schema.org/FoodEstablishment> .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Attività
:Attività rdf:type owl:Class ;
          owl:equivalentClass <http://www.w3.org/ns/prov#Activity> ;
          owl:disjointWith <http://www.w3.org/ns/prov#Agent> ,
                           <http://www.w3.org/ns/prov#Entity> ;
          rdfs:comment "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities"@en ,
                       "Un'attività è qualcosa che si verifica in un periodo di tempo e agisce su o con entità; può includere il consumo, l'elaborazione, la trasformazione, la modifica, il trasferimento, l'utilizzo o la generazione di entità"@it ;
          rdfs:label "Activity"@en ,
                     "Attività"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#AvviareImpresa
:AvviareImpresa rdf:type owl:Class ;
                rdfs:subClassOf :Attività ;
                owl:disjointWith :Consegnare ,
                                 :Consumare ,
                                 :Ordinare ,
                                 :Recensire ;
                rdfs:comment "Attività di inaugurazione di un esercizio alimentare da parte di un proprietario"@it ,
                             "Inauguration of a food establishment by an owner"@en ;
                rdfs:label "Avviare Impresa"@it ,
                           "Start Business"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Bar
:Bar rdf:type owl:Class ;
     rdfs:subClassOf :EsercizioAlimentare ;
     rdfs:comment "Esercizio pubblico in cui si sosta brevemente per consumare generalmente bevande, dolci e cibi leggeri"@it ,
                  "Public exercise where you stop briefly to generally consume drinks, sweets and light foods"@en ;
     rdfs:label "Bar"@it ,
                "Pub"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Città
:Città rdf:type owl:Class ;
       rdfs:subClassOf :LuogoGeografico ;
       owl:disjointWith :Piazza ,
                        :Via ;
       rdfs:comment "Centro abitato formato da un aggregato di costruzioni più o meno pianificato, sorto da un accentramento culturale, economico e amministrativo."@it ,
                    "Inhabited centre formed by a more or less planned aggregate of buildings, which arose from a cultural, economic and administrative centralisation"@en ;
       rdfs:label "Città"@it ,
                  "City"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Cliente
:Cliente rdf:type owl:Class ;
         rdfs:subClassOf :Persona ;
         rdfs:comment "Person who has consumed at least once at a certain food establishment"@en ,
                      "Persona che ha consumato almeno una volta presso un certo esercizio alimentare"@it ;
         rdfs:label "Client"@en ,
                    "Cliente"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Consegnare
:Consegnare rdf:type owl:Class ;
            rdfs:subClassOf :Attività ;
            owl:disjointWith :Consumare ,
                             :Ordinare ,
                             :Recensire ;
            rdfs:comment "Activity of delivering a certain food preparation, previously ordered, to a customer"@en ,
                         "Attività di recapitare ad un cliente una certa portata precedentemente ordinata"@it ;
            rdfs:label "Consegnare"@it ,
                       "Deliver"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Consumare
:Consumare rdf:type owl:Class ;
           rdfs:subClassOf :Attività ;
           owl:disjointWith :Ordinare ,
                            :Recensire ;
           rdfs:comment "Activity that refers to eating a certain ordered food preparation"@en ,
                        "Attività che fa riferimento al mangiare una certa pietanza ordinata"@it ;
           rdfs:label "Consumare"@it ,
                      "Consume"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#EsercizioAlimentare
:EsercizioAlimentare rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
                     owl:disjointWith :Pietanza ,
                                      :Recensione ;
                     rdfs:comment "Attività commerciale a carattere alimentare"@it ,
                                  "Commercial activity in food sector"@en ,
                                  "Person who has consumed in a certain food establishment at least once"@en ;
                     rdfs:label "Esercizio Alimentare"@it ,
                                "Food Establishment"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#FruttiSemplici
:FruttiSemplici rdf:type owl:Class ;
                rdfs:subClassOf :Frutto ;
                rdfs:comment "Formati esclusivamente dall'ovario del singolo fiore."@it ,
                             "Fruit derived from the ripening of a simple or compound ovary in a flower with only one pistil."@en ;
                rdfs:label "Frutti Semplici"@it ,
                           "Simple Fruit"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Frutto
:Frutto rdf:type owl:Class ;
        rdfs:subClassOf :IngredienteVegano ;
        owl:disjointUnionOf ( :FruttiSemplici
                              :FruttoAggregato
                              :Infruttescenza
                            ) ;
        rdfs:comment "Alimento costituito dal complesso delle parti del fiore, e talora di organi vicini, che accompagnano o racchiudono i semi fino alla loro maturità."@it ,
                     "Food formed by the complex of the parts of the flower, and sometimes of neighboring organs, which accompany or enclose the seeds until their maturity."@en ;
        rdfs:label "Fruit"@en ,
                   "Frutto"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#FruttoAggregato
:FruttoAggregato rdf:type owl:Class ;
                 rdfs:subClassOf :Frutto ;
                 rdfs:comment "Frutto derivato da più pistilli dello stesso fiore che rimangono uniti anche nel frutto."@it ,
                              "ruits form from single flowers that have multiple carpels which are not joined together, i.e. each pistil contains one carpel."@en ;
                 rdfs:label "Aggregate Fruit"@en ,
                            "Frutto Aggregato"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#FruttoCarnoso
:FruttoCarnoso rdf:type owl:Class ;
               rdfs:subClassOf :FruttiSemplici ;
               rdfs:comment "Fruits with parenchyma rich in water."@en ,
                            "Frutti con parenchimi ricchi di acqua"@it ;
               rdfs:label "Fleshy Fruit"@en ,
                          "Frutto Carnoso"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#FruttoSecco
:FruttoSecco rdf:type owl:Class ;
             rdfs:subClassOf :FruttiSemplici ;
             rdfs:comment "Fruits in which at maturity all the layers have scarce parenchyma and a rather low water content."@en ,
                          "Frutti in cui a maturità tutti gli strati hanno scarsi parenchimi e un contenuto di acqua piuttosto basso."@it ;
             rdfs:label "Dry Fruit"@en ,
                        "Frutto Secco"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Fungo
:Fungo rdf:type owl:Class ;
       rdfs:subClassOf :IngredienteVegano ;
       rdfs:comment "Heterotrophic, saprophytic or parasitic plant organism, devoid of leaves, flowers, roots, usually of soft and spongy consistency."@en ,
                    "Organismo vegetale eterotrofo, saprofita o parassita, privo di foglie, di fiori, di radici, di consistenza generalmente molle e spugnosa."@it ;
       rdfs:label "Fungo"@it ,
                  "Mushroom"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Infruttescenza
:Infruttescenza rdf:type owl:Class ;
                rdfs:subClassOf :Frutto ;
                rdfs:comment "Insieme dei frutti derivanti dagli ovari di una infiorescenza."@it ,
                             "The ensemble of fruits derived from the ovaries of an inflorescence."@en ;
                rdfs:label "Infructescence"@en ,
                           "Infruttescenza"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#IngredienteNonVegetariano
:IngredienteNonVegetariano rdf:type owl:Class ;
                           rdfs:subClassOf :ingrediente ;
                           owl:disjointWith :IngredienteVegetariano ;
                           rdfs:comment "Any food that is derived from animals, in the sense of compound of animal meat."@en ,
                                        "Qualsiasi alimento che sia derivato da animali, nell'accezione di composto di carne animale"@it ;
                           rdfs:label "Ingrediente Non Vegetariano"@it ,
                                      "Non Vegetarian Ingredient"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#IngredienteVegano
:IngredienteVegano rdf:type owl:Class ;
                   rdfs:subClassOf :IngredienteVegetariano ;
                   owl:disjointWith :Uovo ;
                   rdfs:comment "Any food that is not of animal origin, not even produced by them."@en ,
                                "Qualsiasi alimento che non sia di origine animale, neanche da loro prodotto."@it ;
                   rdfs:label "Ingrediente Vegano"@it ,
                              "Vegan Ingredient"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#IngredienteVegetariano
:IngredienteVegetariano rdf:type owl:Class ;
                        rdfs:subClassOf :ingrediente ;
                        rdfs:comment "Any food that is not of animal origin, in the sense of compound of animal meat."@en ,
                                     "Qualsiasi alimento che non sia di origine animale, nell'accezione di composto di carne animale."@it ;
                        rdfs:label "Ingrediente Vegetariano"@it ,
                                   "Vegetarian Ingredient"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#LuogoGeografico
:LuogoGeografico rdf:type owl:Class ;
                 owl:disjointWith <http://www.w3.org/ns/prov#Agent> ,
                                  <http://www.w3.org/ns/prov#Entity> ;
                 rdfs:comment "Area spaziale univocamente identificato da coordinate geografiche di latitudine e longitudine"@it ,
                              "Spatial area uniquely identified by geographic coordinates of latitude and longitude"@en ;
                 rdfs:label "Geographical Location"@en ,
                            "Luogo Geografico"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Metropoli
:Metropoli rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :haAbitanti ;
                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                      owl:onDatatype xsd:positiveInteger ;
                                                      owl:withRestrictions ( [ xsd:minInclusive "1000000"^^xsd:positiveInteger
                                                                             ]
                                                                           )
                                                    ]
                               ] ;
           rdfs:subClassOf :Città ;
           rdfs:comment "Città di grandi dimensioni con un'area comunale fortemente popolata, centro economico e culturale di una regione o di un Paese, e spesso nodo di comunicazioni internazionali."@it ,
                        "Large city with a heavily populated municipal area, economic and cultural center of a region or country, and often a hub for international communications."@en ;
           rdfs:label "Metropoli"@it ,
                      "Metropolis"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Ordinare
:Ordinare rdf:type owl:Class ;
          rdfs:subClassOf :Attività ;
          owl:disjointWith :Recensire ;
          rdfs:comment "Activity of commissioning a desired food preparation from a certain food establishment"@it ,
                       "Attività di commissionare ad un certo esercizio alimentare una portata desiderata"@it ;
          rdfs:label "Order"@en ,
                     "Ordinare"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Ortaggio
:Ortaggio rdf:type owl:Class ;
          rdfs:subClassOf :IngredienteVegano ;
          rdfs:comment "Insieme di alimenti vegetali, perlopiù coltivati e costituiti da piante intere o da loro parti."@it ,
                       "Set of foods, mostly cultivated and made up of whole plants or their parts."@en ;
          rdfs:label "Ortaggio"@it ,
                     "Vegetable"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Paninoteca
:Paninoteca rdf:type owl:Class ;
            rdfs:subClassOf :EsercizioAlimentare ;
            rdfs:comment "Luogo specializzato nella preparazione di panini, che risultano generalmente le uniche pietanza servite"@it ,
                         "Place specialized in the preparation of sandwiches, which are generally the only food preparation served"@en ;
            rdfs:label "Paninoteca"@it ,
                       "Sandwich and Burger Shop"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Persona
:Persona rdf:type owl:Class ;
         owl:equivalentClass foaf:Person ;
         rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
         rdfs:comment "Individual of the human species"@en ,
                      "Individuo della specie umana."@it ;
         rdfs:label "Person"@en ,
                    "Persona"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Piazza
:Piazza rdf:type owl:Class ;
        rdfs:subClassOf :LuogoGeografico ;
        owl:disjointWith :Via ;
        rdfs:comment "Area libera, limitata in tutto o in parte da costruzioni, con varia funzione urbanistica, all'incrocio di più strade."@it ,
                     "Free area, limited in whole or in part by buildings, with various urban functions, at the intersection of several roads."@en ;
        rdfs:label "Piazza"@it ,
                   "Place"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Pietanza
:Pietanza rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :utilizzaIngrediente ;
                                owl:someValuesFrom :ingrediente
                              ] ;
          rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
          owl:disjointWith :Recensione ;
          rdfs:comment "Result of a series of steps in the preparation of one or more ingredients. In the context of this ontology the offer of restaurants, pizzerias, pubs, etc ..."@en ,
                       "Risultato di una serie di passaggi di preparazione di uno o più ingredienti. Nel contesto di questa ontologia le proposte dei vari ristoranti, pizzerie, pub, ecc..."@it ;
          rdfs:label "Food Preparation"@en ,
                     "Pietanza"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#PietanzaVegetariana
:PietanzaVegetariana rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :utilizzaIngrediente ;
                                                                  owl:someValuesFrom :IngredienteVegetariano
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :utilizzaIngrediente ;
                                                                  owl:allValuesFrom :IngredienteVegetariano
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Pietanza ;
                     rdfs:comment "Risultato della preparazione di soli ingredienti vegetariani"@it ,
                                  "The result of the preparation of only vegetarian ingredients"@en ;
                     rdfs:label "Pietanza Vegetariana"@it ,
                                "Vegetarian Food Preparation"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Pizzeria
:Pizzeria rdf:type owl:Class ;
          rdfs:subClassOf :Ristorante ;
          rdfs:comment "Locale pubblico in cui si preparano e si servono ai clienti pizze e simili."@it ,
                       "Public place in which they prepare and serve to the customers pizzas and similar foods."@en ;
          rdfs:label "Pizzeria"@en ,
                     "Pizzeria"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Recensione
:Recensione rdf:type owl:Class ;
            rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
            rdfs:comment "Commento di esamina e valutazione di una pietanza consumata presso un certo esercizio alimentare"@it ,
                         "Evaluation of a food preparation consumed in a specific food establishment"@en ;
            rdfs:label "Recensione"@it ,
                       "Review"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Recensire
:Recensire rdf:type owl:Class ;
           rdfs:subClassOf :Attività ;
           rdfs:comment "Activity of examining and evaluating a food preparation at a certain food establishment"@en ,
                        "Attività di esaminare e valutare una pietanza consumata presso un certo esercizio alimentare"@it ;
           rdfs:label "Recensire"@it ,
                      "Review"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Recensore
:Recensore rdf:type owl:Class ;
           rdfs:subClassOf :Persona ;
           rdfs:comment "Person who has reviewed the food establishment tried, after a consummation"@en ,
                        "Persona che a seguito di una consumazione ha recensito l'esercizio alimentare provato"@it ;
           rdfs:label "Recensore"@it ,
                      "Reviewer"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Ristorante
:Ristorante rdf:type owl:Class ;
            rdfs:subClassOf :EsercizioAlimentare ;
            rdfs:comment "Esercizio pubblico dove si consumano pasti completi che vengono serviti da camerieri su tavoli disposti in un locale apposito."@it ,
                         "Public exercise where complete meals are consumed, which are served by waiters on tables arranged in a special room."@en ;
            rdfs:label "Restaurant"@en ,
                       "Ristorante"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#Ruolo
:Ruolo rdf:type owl:Class ;
       owl:equivalentClass <http://www.w3.org/ns/prov#Role> ;
       owl:disjointWith <http://www.w3.org/ns/prov#Agent> ,
                        <http://www.w3.org/ns/prov#Entity> ;
       rdfs:comment "Function performed by a person in a certain situation, action ..."@en ,
                    "Funzione svolta da una persona in un determinato ambito, situazione, azione..."@it ;
       rdfs:label "Role"@en ,
                  "Ruolo"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#RuoloCliente
:RuoloCliente rdf:type owl:Class ;
              rdfs:subClassOf :Ruolo ;
              owl:disjointWith :RuoloRecensore ,
                               :RuoloTitolareImpresa ;
