<http://www.w3.org/2004/02/skos/core#prefLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#starRating
:starRating rdf:type rdfs:Datatype ;
            owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                  owl:onDatatype xsd:double ;
                                  owl:withRestrictions ( [ xsd:minInclusive "1.0"^^xsd:double
                                                         ]
                                                         [ xsd:maxInclusive "5.0"^^xsd:double
                                                         ]
                                                       )
                                ] .


#################################################################
#    Object Properties
#################################################################

###  http://schema.org/reviewRating
<http://schema.org/reviewRating> rdf:type owl:ObjectProperty ;
                                 rdfs:domain :Recensione ;
                                 rdfs:range <http://schema.org/Rating> .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#AvviareIlLocale
:AvviareIlLocale rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
                 rdfs:domain :AvviareImpresa ;
                 rdfs:range :EsercizioAlimentare ;
                 rdfs:comment "Property that links the activity of opening a food establishment with the local involved"@en ,
                              "Proprietà che collega l'attività di aprire un esercizio alimentare con il locale coinvolto"@it ;
                 rdfs:label "Avviare Il Locale"@it ,
                            "Start The Food Establishment"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ConsegnareAlCliente
:ConsegnareAlCliente rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
                     rdfs:domain :Consegnare ;
                     rdfs:range :Cliente ;
                     rdfs:comment "Activity that links the activity of delivering an order to the customer who made that order"@en ,
                                  "Attività che collega l'attività di consegnare un ordine al cliente che ha fatto quell'ordine"@it ;
                     rdfs:label "Consegnare Al Cliente"@it ,
                                "Deliver To The Customer"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ConsegnareOrdine
:ConsegnareOrdine rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
                  rdfs:domain :Consegnare ;
                  rdfs:range :Ordine ;
                  rdfs:comment "Property that links the delivering activity with the order to be delivered"@en ,
                               "Proprietà che collega l'attività di consegnare con l'ordine che deve essere consegnato"@it ;
                  rdfs:label "Consegnare Ordine"@it ,
                             "Deliver The Order"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ConsegnatoDa
:ConsegnatoDa rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAssociatedWith> ;
              rdfs:domain :Consegnare ;
              rdfs:range :Fattorino ;
              rdfs:comment "Property that links the delivery activity with the person who has to do the delivery"@en ,
                           "Proprietà che collega l'attività di consegnare con la persona che deve fare la consegna"@it ;
              rdfs:label "Consegnato Da"@it ,
                         "Delivered By"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#EStatoRecensitoDa
:EStatoRecensitoDa rdf:type owl:ObjectProperty ;
                   owl:inverseOf :HaFattoRecensioneAlLocale ;
                   rdfs:comment "Property that links a food establishment with the person who reviewed it"@en ,
                                "Proprietà che collega un esercizio alimentare con la persona che l'ha recensito"@it ;
                   rdfs:label "E Stato Recensito Da"@it ,
                              "Has Been Reviewed By"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#FrittoCon
:FrittoCon rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :utilizzaIngrediente ;
           rdfs:domain :Pietanza ;
           rdfs:range :ingrediente ;
           rdfs:comment "Property that indicates with which ingredients the food preparation has been deep fried."@en ,
                        "Proprietà che indica con quali ingredienti la pietanza è stata fritta."@it ;
           <http://www.w3.org/2004/02/skos/core#altLabel> "Deep fried using"@en ,
                                                          "Deep fried with"@en ,
                                                          "Fritto in"@it ,
                                                          "Fritto usando"@it ;
           <http://www.w3.org/2004/02/skos/core#prefLabel> "Deep fried in"@en ,
                                                           "Fritto con"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaAvviatoImpresa
:HaAvviatoImpresa rdf:type owl:ObjectProperty ;
                  owl:inverseOf :LocaleAvviatoDa ;
                  owl:propertyChainAxiom ( [ owl:inverseOf :ImpresaAvviataDa
                                           ]
                                           :AvviareIlLocale
                                         ) ;
                  rdfs:comment "Property that links the person with the food establishments that he has started"@en ,
                               "Proprietà che collega la persona con i vari esercizi alimentari che ha avviato"@it ;
                  rdfs:label "Ha Avviato Impresa"@it ,
                             "Has Opened The Food Establishment"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaConsegnatoOrdine
:HaConsegnatoOrdine rdf:type owl:ObjectProperty ;
                    owl:inverseOf :OrdineConsegnatoDa ;
                    owl:propertyChainAxiom ( [ owl:inverseOf :ConsegnatoDa
                                             ]
                                             :ConsegnareOrdine
                                           ) ;
                    rdfs:comment "Property that links the delivery guy with the orders delivered"@en ,
                                 "Proprietà che collega il fattorino con gli ordini che ha consegnato"@it ;
                    rdfs:label "Ha Consengato Ordine"@it ,
                               "Has Delivered Order"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaFattoRecensioneAlLocale
:HaFattoRecensioneAlLocale rdf:type owl:ObjectProperty ;
                           owl:propertyChainAxiom ( [ owl:inverseOf :recensitoDa
                                                    ]
                                                    :recensireLocale
                                                  ) ;
                           rdfs:comment "Property that links the person to the place they reviewed"@en ,
                                        "Proprietà che collega la persona al locale di cui ha fatto una recensione"@it ;
                           rdfs:label "Ha Fatto Recensione Al Locale"@it ,
                                      "Has Reviewed The Food Establishment"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaOrdinatoDalLocale
:HaOrdinatoDalLocale rdf:type owl:ObjectProperty ;
                     owl:inverseOf :HaRicevutoOrdineDa ;
                     owl:propertyChainAxiom ( [ owl:inverseOf :ordinatoDa
                                              ]
                                              :ordinareDalLocale
                                            ) ;
                     rdfs:comment "Property that connects the person to the place where he placed an order"@en ,
                                  "Proprietà che collega la persona al locale presso cui ha fatto un ordine"@it ;
                     rdfs:label "Ha Ordinato Dal Locale"@it ,
                                "Has Ordered From The Food Establishment"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaOrdinatoPietanza
:HaOrdinatoPietanza rdf:type owl:ObjectProperty ;
                    owl:propertyChainAxiom ( :haFattoOrdine
                                             :OrdineCompostaDaPietanza
                                           ) ;
                    rdfs:comment "Property that links the person to the food preparation that makes up one of his orders"@en ,
                                 "Proprietà che collega la persona alla pietanza che compone uno dei suoi ordini"@it ;
                    rdfs:label "Ha Ordinato Pietanza"@it ,
                               "Has Ordered Food Preparation"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#HaRicevutoOrdineDa
:HaRicevutoOrdineDa rdf:type owl:ObjectProperty ;
                    rdfs:comment "Property that links the food establishment with the customer who placed an order"@en ,
                                 "Proprietà che collega il locale con il cliente che ha effettuato un ordine"@it ;
                    rdfs:label "Ha Ricevuto Ordine Da"@it ,
                               "Has Received Order By"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ImpresaAvviataDa
:ImpresaAvviataDa rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                  rdfs:domain :AvviareImpresa ;
                  rdfs:range :TitolareImpresa ;
                  rdfs:comment "Property that links the starting a new food establishment with the corrispective owner"@en ,
                               "Proprietà che collega l'attività di avviare un impresa con il corrispettivo proprietario"@it ;
                  rdfs:label "Food Establishment Started By"@en ,
                             "Impresa Avviata Da"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#LocaleAvviatoDa
:LocaleAvviatoDa rdf:type owl:ObjectProperty ;
                 rdfs:comment "Property that links the food establishment with the owner"@en ,
                              "Proprietà che collega il locale con il proprietario"@it ;
                 rdfs:label "Food Establishment Started By"@en ,
                            "Locale Avviato Da"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#LocaleServePietanza
:LocaleServePietanza rdf:type owl:ObjectProperty ;
                     owl:inverseOf :PietanzaServitaInLocale ;
                     rdfs:domain :EsercizioAlimentare ;
                     rdfs:range :Pietanza .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#OrdineCompostaDaPietanza
:OrdineCompostaDaPietanza rdf:type owl:ObjectProperty ;
                          rdfs:domain :Ordine ;
                          rdfs:range :Pietanza ;
                          rdfs:comment "Property that links the order to the food preparations that compose it"@en ,
                                       "Proprietà che lega l'ordine alle pietanza che lo compongono"@it ;
                          rdfs:label "Order Consisting Of Food Preparation"@en ,
                                     "Ordine Composto Da Pietanza"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#OrdineConsegnatoDa
:OrdineConsegnatoDa rdf:type owl:ObjectProperty ;
                    rdfs:comment "Property that links the order to the delivery guy who delivered it"@en ,
                                 "Proprietà che collega l'ordine al fattorino che l'ha consegnato"@it ;
                    rdfs:label "Order Deliverd By"@en ,
                               "Ordine Consegnato Da"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#PietanzaServitaInLocale
:PietanzaServitaInLocale rdf:type owl:ObjectProperty ;
                         rdfs:domain :Pietanza ;
                         rdfs:range :EsercizioAlimentare .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#componeIngredienteDerivato
:componeIngredienteDerivato rdf:type owl:ObjectProperty ;
                            owl:inverseOf :ingredienteDerivatoDa ;
                            rdfs:domain :ingrediente ;
                            rdfs:range :IngredienteDerivato ;
                            rdfs:comment "Property that indicates which derived ingredients use this ingredient."@en ,
                                         "Proprietà che indica quali ingredienti derivati utilizzano questo ingrediente."@it ;
                            rdfs:label "Compone ingrediente derivato"@it ,
                                       "Part of derived ingredient"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#haFattoOrdine
:haFattoOrdine rdf:type owl:ObjectProperty ;
               owl:propertyChainAxiom ( [ owl:inverseOf :ordinatoDa
                                        ]
                                        [ owl:inverseOf :ordineCreatoDaAzione
                                        ]
                                      ) ;
               rdfs:comment "Property that links the person with the order made"@en ,
                            "Proprietà che collega la persona con l'ordine che ha fatto"@it ;
               rdfs:label "Ha Fatto Ordine"@it ,
                          "Has Made Order"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#haFattoRecensione
:haFattoRecensione rdf:type owl:ObjectProperty ;
                   owl:inverseOf :recensioneFattaDa ;
                   rdfs:comment "Property that links the person to the review made"@en ,
                                "Proprietà che collega la persona alla recensione che ha fatto"@it ;
                   rdfs:label "Ha Fatto Recensione"@it ,
                              "Has Made The Review"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#haRuolo
:haRuolo rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.w3.org/ns/prov#hadRole> ;
         rdfs:domain :Persona ;
         rdfs:range <http://www.w3.org/ns/prov#Role> ;
         rdfs:comment "Property that connects a person to the role they play in the context of a particular activity"@en ,
                      "Proprietà che collega una persona al ruolo che svolge nel contesto di una particolare attivitià"@it ;
         rdfs:label "Ha Ruolo"@it ,
                    "Has Role"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain collectionentity:Collection ;
           rdfs:range owl:Thing ;
           rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)."@en ,
                        "The link to the members of a Set"@en ;
           rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
           rdfs:label "has member"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ingredienteDerivatoDa
:ingredienteDerivatoDa rdf:type owl:ObjectProperty ;
                       rdfs:domain :IngredienteDerivato ;
                       rdfs:range :ingrediente ;
                       rdfs:comment """Property that specifies from which ingredient another ingredient derives.
When used multiple times for the same individual, it indicates that an ingredient is made up of multiple ingredients."""@en ,
                                    """Proprietà che specifica da quale ingrediente deriva un altro ingrediente.
Se usata multiple volte per un individuo, indica che un ingrediente è composto da più ingredienti."""@it ;
                       rdfs:label "Ingredient derived from"@en ,
                                  "Ingrediente derivato da"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ingredienteIncompatibileConDisturbo
:ingredienteIncompatibileConDisturbo rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf owl:topObjectProperty ;
                                     rdfs:domain :ingrediente ;
                                     rdfs:range :Disturbo ;
                                     owl:propertyChainAxiom ( :ingredienteDerivatoDa
                                                              :ingredienteIncompatibileConDisturbo
                                                            ) ;
                                     rdfs:comment """Property that associates an ingredient with the disorders that a person may have, thus discouraging its intake.
Ingredients that derive from other ingredients inherit this property."""@en ,
                                                  """Proprietà che associa un ingrediente ai disturbi che una persona può avere, sconsigliandone l'assunzione.
La proprietà è ereditata dagli ingredienti derivanti da multipli ingredienti."""@it ;
                                     rdfs:label "Ingredient incompatible with disorder"@en ,
                                                "Ingrediente incompatibile con disturbo"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain owl:Thing ;
            rdfs:range collectionentity:Collection ;
            rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
            rdfs:label "is member of"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#localeGestisceOrdine
:localeGestisceOrdine rdf:type owl:ObjectProperty ;
                      owl:propertyChainAxiom ( [ owl:inverseOf :ordinareDalLocale
                                               ]
                                               [ owl:inverseOf :ordineCreatoDaAzione
                                               ]
                                             ) ;
                      rdfs:comment "Property that links the room to the orders they have managed"@en ,
                                   "Proprietà che collega il locale agli ordini che hanno gestito"@it ;
                      rdfs:label "Food Establishment Handles Order"@en ,
                                 "Locale Gestisce Ordine"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#localeHaRecensione
:localeHaRecensione rdf:type owl:ObjectProperty ;
                    owl:propertyChainAxiom ( [ owl:inverseOf :recensireLocale
                                             ]
                                             [ owl:inverseOf :recensioneCreataDaAzione
                                             ]
                                           ) ;
                    rdfs:comment "Property chain che collega direttamente l'esercizio alimentare alle recensioni che ha ricevuto"@it ,
                                 "Property chain that links the food establishment directly to the reviews it has received"@en ;
                    rdfs:label "Local Has Review"@en ,
                               "Locale Ha Recensione"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#localeSituatoIn
:localeSituatoIn rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :EsercizioAlimentare ;
                 rdfs:range :LuogoGeografico ;
                 rdfs:comment "Property that allows a food establishment to be geographically and uniquely localized"@en ,
                              "Proprietà che permette di localizzare geograficamente ed univocamente un esercizio alimentare"@it ;
                 rdfs:label "Local Located In"@en ,
                            "Locale Situato In"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ordinareDalLocale
:ordinareDalLocale rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
                   rdfs:domain :Ordinare ;
                   rdfs:range :EsercizioAlimentare ;
                   rdfs:comment "Property that links the activity of ordering with the food establishment where the order was placed"@en ,
                                "Proprietà che collega l'attività di ordinare con il locale presso cui si è fatto l'ordine"@it ;
                   rdfs:label "Order From The Food Establishment"@en ,
                              "Ordinare Dal Locale"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ordinatoDa
:ordinatoDa rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAssociatedWith> ;
            rdfs:domain :Ordinare ;
            rdfs:range :Cliente ;
            rdfs:comment "Property that links the order activity with the person that made the order"@en ,
                         "Proprietà che collega l'attività ordinare con la persona che effettua l'ordine"@it ;
            rdfs:label "Order By"@en ,
                       "Ordinato Da"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#ordineCreatoDaAzione
:ordineCreatoDaAzione rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasGeneratedBy> ;
                      rdfs:domain :Ordine ;
                      rdfs:range :Ordinare ;
                      rdfs:comment "Property that links the order to the activity that created it"@en ,
                                   "Proprietà che collega l'ordine all'azione ordinare che l'ha creato"@it ;
                      rdfs:label "Order Created By Activity"@en ,
                                 "Ordine Creato Da Azione"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#pietanzaIncompatibileConDisturbo
:pietanzaIncompatibileConDisturbo rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdfs:domain :Pietanza ;
                                  rdfs:range :Disturbo ;
                                  owl:propertyChainAxiom ( :utilizzaIngrediente
                                                           :ingredienteIncompatibileConDisturbo
                                                         ) ;
                                  rdfs:comment """Property that associates a preparation with the ailments that a person may have, not recommending its intake.
A preparation inherits all the incompatibilities of the ingredients that compose it."""@en ,
                                               """Proprietà che associa una preparazione ai disturbi che una persona può avere, sconsigliandone l'assunzione.
Una preparazione eredita tutte le incompatibilità degli ingredienti che la compongono."""@it ;
                                  rdfs:label "Food preparation incompatible with disorder"@en ,
                                             "Pietanza incompatibile con disturbo"@it .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#recensioneCreataDaAzione
:recensioneCreataDaAzione rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasGeneratedBy> ;
                          rdfs:domain :Recensione ;
                          rdfs:range :Recensire ;
                          rdfs:comment "Property that links the review to the action from which it is created"@en ,
                                       "Proprietà che collega la recensione all'azione dalla quale viene creata"@it ;
                          rdfs:label "Recensione Creata Da Azione"@it ,
                                     "Review Created From Activity"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#recensioneFattaDa
:recensioneFattaDa rdf:type owl:ObjectProperty ;
                   owl:propertyChainAxiom ( :recensioneCreataDaAzione
                                            :recensitoDa
                                          ) ;
                   rdfs:comment "Property chain che collega la recensione alla persona che l'ha fatta"@it ,
                                "Property chain that links the review to the person who did it"@en ;
                   rdfs:label "Recensione Fatta Da"@it ,
                              "Review Made By"@en .


###  http://www.semanticweb.org/progettomodsem2020/deliverydoctor#recensireLocale
:recensireLocale rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
                 rdfs:domain :Recensire ;
                 rdfs:range :EsercizioAlimentare ;
                 rdfs:comment "Property that links the activity of reviewing with the food establishment under review"@en ,
                              "Proprietà che permette di collegare l'attività di recensire con l'esercizio alimentare oggetto della recensione"@it ;
                 rdfs:label "Recensire Locale"@it ,
                            "Review Food Establishment"@en .


