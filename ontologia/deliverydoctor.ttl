@prefix : <http://www.semanticweb.org/progettomodsem2020/deliverydoctor#> .
@prefix icd: <http://purl.bioontology.org/ontology/ICD10CM/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/progettomodsem2020/deliverydoctor> .

<http://www.semanticweb.org/progettomodsem2020/deliverydoctor> rdf:type owl:Ontology ;
                                                                <http://purl.org/dc/elements/1.1/creator> "Lorenzo Sciandra"^^xsd:string ,
                                                                                                          "Stefano Vittorio Porta"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/date> "2020-12-29T18:30:10"^^xsd:dateTime ;
                                                                <http://purl.org/dc/elements/1.1/description> "Ontologia che tratta esercizi alimentari, pietanze servite ed interazione con utenti al fine dell'ordine, consegna e consumo di queste in precisi luoghi geografici."@it ;
                                                                <http://purl.org/dc/elements/1.1/language> "italiano"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/subject> "food delivery"^^xsd:string ,
                                                                                                          "ontology"^^xsd:string ;
                                                                <http://purl.org/dc/elements/1.1/title> "Delivery Doctor"^^xsd:string ;
                                                                vann:preferedNamespacePrefix "deliverydr"^^xsd:string ;
                                                                vann:preferredNamespaceUri "http://www.semanticweb.org/progettomodsem2020/deliverydoctor"^^xsd:string ;
                                                                rdfs:label "deliverydr"^^xsd:string .

